{
  "name": "connexto-axiom-agents",
  "version": "0.1.0",
  "description": "Operational execution system where AI agents work continuously to advance real objectives",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/main.ts",
    "build": "tsc",
    "start": "node --env-file=.env dist/src/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bot": "tsx watch --env-file=.env src/bot.ts",
    "test": "node --import tsx --test '**/*.test.ts'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "openclaw": "openclaw gateway --auth token --token dev-local",
    "test:vitest": "vitest run"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "pino": "^10.3.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.4.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}